<div class="container">
  <div class="row justify-content-between">
    <% @interviews.each do |interview| %>
      <div class="card col-12 col-sm-6 col-md-4 col-lg-3" style="width: 18rem; height: 20rem">
        <%= link_to interview.user.nickname, interview_path(interview), class: "stretched-link" %>
      </div>
    <% end %>
  </div>
</div>
